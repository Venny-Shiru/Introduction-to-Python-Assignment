x = float(input('Enter the first number: '))
y= float(input('Enter the second number: '))
z= input('Enter the operator (+, -, *, /): ')
if z == '+':
    print(f"{x} + {y} = {x + y}")
elif z == '-':
    print(f"{x} - {y} = {x - y}")
elif z == '*':
    print(f"{x} * {y} = {x * y}")
elif z == '/':
    print(f"{x} / {y} = {x / y}")
else:
    print("Invalid input. Please try again")
